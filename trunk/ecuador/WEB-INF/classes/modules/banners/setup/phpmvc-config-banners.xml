<!-- Module Banners-->
	<action-mappings module="Banners">

		<action  path = "bannersList"
						 type = "BannersListAction"
						 name = "bannersList"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="BannersList.tpl"/>
		</action>

		<action  path = "bannersEdit"
						 type = "BannersEditAction"
						 name = "bannersEdit"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="BannersEdit.tpl" />
			<forward  name="failure" path="/Main.php?do=bannersList&amp;message=notget" redirect="true"/>
		</action>

		<action  path = "bannersDoEdit"
						 type = "BannersDoEditAction"
						 name = "bannersDoEdit"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="/Main.php?do=bannersList&amp;message=saved" redirect="true"/>
			<forward  name="failure" path="/Main.php?do=bannersList&amp;message=notsaved" redirect="true"/>
		</action>

		<action  path = "bannersDoDelete"
						 type = "BannersDoDeleteAction"
						 name = "bannersDoDelete"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="/Main.php?do=bannersList&amp;message=deleted" redirect="true" />
			<forward  name="failure" path="/Main.php?do=bannersList&amp;message=notdeleted" redirect="true" />
		</action>

		<action  path = "bannersContentStream"
						 type = "BannersContentStreamAction"
						 name = "bannersContentStream"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="BannersContentStream.tpl" />
		</action>

		<action  path = "bannersPreview"
						 type = "BannersPreviewAction"
						 name = "bannersPreview"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="BannersDisplay.tpl" />
		</action>

		<action  path = "bannersZonesDisplay"
						 type = "BannersZonesDisplayAction"
						 name = "bannersZonesDisplay"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="BannersZonesDisplay.tpl" />
			<forward  name="include" path="BannersZonesDisplayInclude.tpl" />
		</action>

		<action  path = "bannersDoClickThru"
						 type = "BannersDoClickThruAction"
						 name = "bannersDoClickThru"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="BannersDoClickThru.tpl" />
		</action>

		<action  path = "bannersStatsShow"
						 type = "BannersStatsShowAction"
						 name = "bannersStatsShow"
						scope = "request"
				 validate = "true">
			<forward  name="success" path="BannersStatsShow.tpl" />
		</action>

