<!-- Section Users -->

		<action      path = "affiliatesUsersList"
								 type = "AffiliatesUsersListAction"
								 name = "affiliatesUsersList"
								scope = "request"
						 validate = "true">
				<forward name="success" path="AffiliatesUsersList.tpl"/>
		</action>

		<action      path = "affiliatesUsersDoEditUser"
								 type = "AffiliatesUsersDoEditUserAction"
								 name = "affiliatesUsersDoEditUser"
								scope = "request"
						 validate = "true">
			<forward name="success" path="/Main.php?do=affiliatesUsersList&amp;message=saved" redirect="true" />
			<forward name="failure" path="AffiliatesUsersList.tpl" />
		</action>

		<action      path = "affiliatesUsersDoDelete"
								 type = "AffiliatesUsersDoDeleteAction"
								 name = "affiliatesUsersDoDelete"
								scope = "request"
						 validate = "true">
			<forward name="success" path="/Main.php?do=affiliatesUsersList&amp;message=deleted" redirect="true" />
		</action>

		<action      path = "affiliatesUsersLogin"
								 type = "AffiliatesUsersLoginAction"
								 name = "affiliatesUsersLogin"
								scope = "request"
						 validate = "true">
				<description value="Modulo de ingreso"/>
				<forward name="success" path="AffiliatesUsersLogin.tpl"/>
				<forward name="success-unified" path="/Main.php?do=usersLogin" redirect="true"/>
		</action>

		<action      path = "affiliatesUsersDoLogin"
								 type = "AffiliatesUsersDoLoginAction"
								 name = "affiliatesUsersDoLogin"
								scope = "request"
						 validate = "true">
				<description value="Modulo de ingreso"/>
				<forward name="success" path="AffiliatesUsersWelcome.tpl"/>
				<forward name="failure-unified" path="UsersLogin.tpl"/>
				<forward name="failure" path="AffiliatesUsersLogin.tpl"/>
		</action>

		<action      path = "affiliatesUsersWelcome"
								 type = "AffiliatesUsersWelcomeAction"
								 name = "affiliatesUsersWelcome"
								scope = "request"
						 validate = "true">
				<forward name="success" path="AffiliatesUsersWelcome.tpl"/>
		</action>

		<action      path = "affiliatesUsersDoLogout"
								 type = "AffiliatesUsersDoLogoutAction"
								 name = "affiliatesUsersDoLogout"
								scope = "request"
						 validate = "true">
				<forward name="success-unified" path="/Main.php?do=usersLogin" redirect="true" />
				<forward name="success" path="/Main.php?do=affiliatesUsersLogin" redirect="true" />
		</action>

		<action    path = "affiliatesUsersDoAddToGroup"
							 type = "AffiliatesUsersDoAddToGroupAction"
							 name = "affiliatesUsersDoAddToGroup"
							scope = "request"
					 validate = "true">
