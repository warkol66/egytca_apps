			<forward name="failure" path="/Main.php?do=affiliatesUsersList&amp;message=notactivated" redirect="true" />
		</action>

		<action    path = "affiliatesUsersDoRemoveFromGroup"
							 type = "AffiliatesUsersDoRemoveFromGroupAction"
