				<height>400</height>
			</resize>
			<jpegQuality>65</jpegQuality>
			<thumbnail>
				<resizeFormat>1</resizeFormat>
				<width>150</width>
				<height>105</height>
			</thumbnail>
		</image>
		<video>
			<resize>
