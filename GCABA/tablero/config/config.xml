<?xml version="1.0" encoding="utf8"?>
<config>
	<system>
		<parameters>
			<debugMode>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</debugMode>
			<mailCapture>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</mailCapture>
			<underMaintenance>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</underMaintenance>
			<maintenanceMessage>El sistema permanecerá en mantenimiento por 2 horas, por favor regrese mas tarde.</maintenanceMessage>
			<hasUnifiedUsernames>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</hasUnifiedUsernames>
			<doLog>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</doLog>
			<applicationTimeZoneGMT>
				<value>-3</value>
				<element_type>TIMEZONE</element_type>
			</applicationTimeZoneGMT>
			<dateFormat>
				<value>d-m-Y</value>
				<element_type>Options</element_type>
				<element_options>
					<option_1>d-m-Y</option_1>
					<option_2>m-d-Y</option_2>
					<option_3>d-m-y</option_3>
					<option_4>m-d-y</option_4>
				</element_options>
			</dateFormat>
			<thousandsSeparator>.</thousandsSeparator>
			<decimalSeparator>,</decimalSeparator>
			<numberOfDecimals>2</numberOfDecimals>
			<siteName>Gobierno de la Ciudad Autónoma de Buenos Aires</siteName>
			<siteDescription>Sistema de Gestión de Proyectos del Gobierno de la Ciudad Autónoma de Buenos Aires</siteDescription>
			<siteShortName>planificacion</siteShortName>
			<webmasterMail>infocippec@cippec.org </webmasterMail>
			<debugMail>dbugger.modulnet@gmail.com</debugMail>
			<fromEmail>infocippec@cippec.org</fromEmail>
			<tablePrefix></tablePrefix>
			<news>Novedades: Bienvenido al Sistema de Gestión de Planificación del Gobierno de la Ciudad Autónoma de Buenos Aires</news>
		</parameters>
		<errorReporting>
			<value>E_ALL &amp; ~E_NOTICE ~E_WARNING</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1>NONE</option_1>
				<option_2>E_ALL &amp; ~E_NOTICE</option_2>
				<option_3>E_ALL</option_3>
				<option_4>E_STRICT</option_4>
				<option_5>E_ALL &amp; E_STRICT</option_5>
				<option_6>E_ALL &amp; ~E_NOTICE ~E_WARNING</option_6>
			</element_options>
		</errorReporting>
		<rowsPerPage>25</rowsPerPage>
		<language>esp</language>
	</system>
	<documents>
		<rowsPerPage>10</rowsPerPage>
		<documentsPath>WEB-INF/documents</documentsPath>
		<useGlobalCategories>
			<value>NO</value>
			<element_type>YES/NO</element_type>
		</useGlobalCategories>
		<usesCategoriesGroupPermission>
			<value>NO</value>
			<element_type>YES/NO</element_type>
		</usesCategoriesGroupPermission>
		<usePasswords>
			<value>NO</value>
			<element_type>YES/NO</element_type>
		</usePasswords>
	</documents>
	<users>
		<documentTypes>
			<dni>1</dni>
			<cuil>2</cuil>
			<otro>3</otro>
		</documentTypes>
	</users>
	<email>
		<mode>
			<value>SMTP</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1>PHP</option_1>
				<option_2>SMTP</option_2>
			</element_options>
		</mode>
		<smtpOptions>
			<smtpHost>mail.anmagaexport.modulserver.com.ar</smtpHost>
			<smtpPort>587</smtpPort>
			<smtpUsername>anmagaex</smtpUsername>
			<smtpPassword>Anmaga</smtpPassword>
			<smtpSsl>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</smtpSsl>
		</smtpOptions>
	</email>
</config>
