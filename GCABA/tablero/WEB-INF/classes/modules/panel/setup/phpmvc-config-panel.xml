<!-- Module Panel -->
	<action-mappings>
		<action    path = "panelBudgetRelationsDoRequest"
							 type = "PanelBudgetRelationsDoRequestAction"
							 name = "panelBudgetRelationsDoRequest"
							scope = "request"
					 validate = "true">
			<forward name="success" path="/Main.php?do=panelBudgetRelationsRequest" redirect="true"/>
			<forward name="failure" path="/Main.php?do=panelBudgetRelationsRequest" redirect="true"/>
		</action>
		
		<action    path = "panelBudgetRelationsDoUpdateX"
							 type = "PanelBudgetRelationsDoUpdateXAction"
							 name = "panelBudgetRelationsDoUpdateX"
							scope = "request"
					 validate = "true">
			<forward name="success" path="PanelBudgetRelationsDoUpdateX.tpl" />
			<forward name="failure" path="PanelBudgetRelationsDoUpdateX.tpl" />
		</action>

	</action-mappings>
<!-- End Module Panel -->
