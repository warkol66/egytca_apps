<?xml version="1.0" encoding="utf8"?>
<config>
	<system>
		<parameters>
			<debugMode>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</debugMode>
			<developmentMode>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</developmentMode>
			<mailCapture>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</mailCapture>
			<underMaintenance>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</underMaintenance>
			<maintenanceMessage>El sistema permanecer√° en mantenimiento por 2 horas, por favor regrese mas tarde.</maintenanceMessage>
			<affiliateUserLoginUnified>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</affiliateUserLoginUnified>
			<hasUnifiedUsernames>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</hasUnifiedUsernames>
			<doLog>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</doLog>
			<applicationTimeZoneGMT>
				<value>-3</value>
				<element_type>TIMEZONE</element_type>
			</applicationTimeZoneGMT>
			<dateFormat>
				<value>d-m-Y</value>
				<element_type>Options</element_type>
				<element_options>
					<option_1>d-m-Y</option_1>
					<option_2>m-d-Y</option_2>
					<option_3>d-m-y</option_3>
					<option_4>m-d-y</option_4>
				</element_options>
			</dateFormat>
			<dateTimeFormat>
				<value>d-m-Y H:M</value>
				<element_type>Options</element_type>
				<element_options>
					<option_1>d-m-Y H:M</option_1>
					<option_2>m-d-Y H:M:S</option_2>
					<option_3>d-m-y H:M</option_3>
					<option_4>m-d-y H:M:S</option_4>
				</element_options>
			</dateTimeFormat>
			<thousandsSeparator>.</thousandsSeparator>
			<decimalSeparator>,</decimalSeparator>
			<numberOfDecimals>2</numberOfDecimals>
			<siteName>Sistema de Seguimiento de Prensa</siteName>
			<siteDescription>Sistema de Seguimiento de Prensa del Gobierno de la Ciudad de Buenos Aires</siteDescription>
			<siteShortName>sistema</siteShortName>
			<webmasterMail>moddebugger@gmail.com</webmasterMail>
			<debugMail>moddebugger@gmail.com</debugMail>
			<fromEmail>moddebugger@gmail.com</fromEmail>
			<tablePrefix></tablePrefix>
			<news>Novedades: Bienvenido al Sistema</news>
			<affiliateNews>Novedades: Bienvenido al Sistema</affiliateNews>
		</parameters>
		<errorReporting>
			<value>E_ALL</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1></option_1>
				<option_2>E_ALL ^ E_NOTICE ^ E_WARNINGS</option_2>
				<option_3>E_ALL</option_3>
				<option_4>E_STRICT</option_4>
			</element_options>
		</errorReporting>
		<rowsPerPage>25</rowsPerPage>
		<language>esp</language>
	</system>
	<email>
		<mode>
			<value>SMTP</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1>PHP</option_1>
				<option_2>SMTP</option_2>
			</element_options>
		</mode>
		<smtpOptions>
			<smtpHost>mail.anmagaexport.modulserver.com.ar</smtpHost>
			<smtpPort>587</smtpPort>
			<smtpUsername>anmagaex</smtpUsername>
			<smtpPassword>Anmaga</smtpPassword>
			<smtpSsl>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</smtpSsl>
		</smtpOptions>
	</email>
	<documents>
		<rowsPerPage>10</rowsPerPage>
		<documentsPath>WEB-INF/documents</documentsPath>
		<useGlobalCategories>
			<value>NO</value>
			<element_type>YES/NO</element_type>
		</useGlobalCategories>
		<usesCategoriesGroupPermission>
			<value>NO</value>
			<element_type>YES/NO</element_type>
		</usesCategoriesGroupPermission>
		<usePasswords>
			<value>NO</value>
			<element_type>YES/NO</element_type>
		</usePasswords>
		<newestLimit>6</newestLimit>
	</documents>
	<clippings>
		<quality>94</quality>
		<width>1024</width>
		<height>800</height>
		<maxDisplayableWidth>800</maxDisplayableWidth>
		<maxDisplayableHeight>600</maxDisplayableHeight>
	</clippings>
	<headlines>
		<maxParsedResults>10</maxParsedResults>
	</headlines>
</config>
